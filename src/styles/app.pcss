
#testRecorderUI {
  position: absolute;
  bottom: 10px;
  left: 10px;
  width: 250px;
  color: white;
  border: 1px solid #0b97c4;
  height: 350px;
  background-color: rgba(11, 11, 11, 0.6);
  border-radius: 4px;
  font-size: 8px;
  resize: both;
  z-index: 1000;
  resize: both;
  overflow: auto;


  .header {
    background-color: rgba(204, 250, 255, 0.9);
    padding: 2px;
    pointer-events: auto;
    line-height: 18px;
    text-align: center;
  }

  select {
    width: 110px;
    height: 18px;
  }
  button {
    height: 18px;
  }
  #clear, #debug, .info {
    cursor: pointer;
    font-size: 13px;
    padding: 2px;
  }
  .info {
    float: right;
  }
  #generatedScript {
    padding: 4px;
    pointer-events: none;

  }
}

